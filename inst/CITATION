citHeader("To cite autograph in publications use:")

bibentry(
  bibtype    = "Manual",
  title    = "autograph: Automatic Plotting of Many Graphs",
  author   = "James Hollway",
  year     = "2025",
  url      = "https://github.com/stocnet/autograph",
  doi      = "10.5281/zenodo.17072231",
  textVersion = paste("James Hollway (2025) ‘autograph: Automatic Plotting of Many Graphs’."
  )
)
